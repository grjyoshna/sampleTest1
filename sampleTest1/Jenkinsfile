pipeline{
    
    agent any
   tools {
        maven 'M3'
        jdk 'JAVA_HOME'
    }
    stages{
         
       stage('Build') { 
            steps { 
               echo 'This is a minimal pipeline.' 
            }
        }
        stage('Compile')
        {
         steps {
            
                bat'mvn clean install'
            
        }
        }
      
    }
}

pipeline{
    environment{
        registry = "grjyoshna/docker_test"
        registryCredential = "dockerhub"
    }
    agent none
        stages {     
            stage('Maven Install') {
                                  agent {         
                                     docker {          
                                          image 'maven:3.5.0'         
                                          }       
                                        }       
                 steps {
                   sh 'mvn clean install'
                     }
             }
        }
        
       
 
      
    
}

pipeline{
    
    agent any
   
    stages{
       
        
            stage('Checkout code') {
                 steps {
                 checkout scm
                 }
             }
        
        stage ('Build') {
            steps {
                sh 'mvn -Dmaven.test.failure.ignore=true install' 
            }
            
        }
        
       
       
      
    }
}

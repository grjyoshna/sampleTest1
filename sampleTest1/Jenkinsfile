pipeline{
    
    agent any
   tools {
        maven 'M3'
        jdk 'JAVA_HOME'
    }
    stages{
          stage ('Initialize') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                ''' 
            }
        }
       
         stage ('Build') {
            steps {
                sh 'mvn -f sampleTest1/pom.xml clean install'
            }
           
        }
      
    }
}

pipeline{
    
    agent any
   tools {
        maven 'M3'
        jdk 'JAVA_HOME'
    }
    stages{
         
       stage('Build') { 
            steps { 
               echo 'This is a minimal pipeline.' 
            }
        }
        stage('Compile')
        {
         steps {
            withMaven(maven : 'apache-maven-3.6.3') {
                bat'mvn clean compile'
            }
        }
        }
      
    }
}
